<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.leo.springbootmybatisdemo.mapper.UserMapper">

    <select id="selectAll" resultType="com.leo.springbootmybatisdemo.entity.User">
      select
        id,
        name,
        age,
        address,
        memo,
        create_time `createTime`
      from
        user
    </select>


	
</mapper>